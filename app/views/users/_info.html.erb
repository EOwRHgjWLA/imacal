<table class='table'>
  <tr><%= image_tag user.get_profile_image, size:'100x100' %></tr>
  <tr>
	  <th>name</th>
	  <th><%= user.display_name %></th>
  </tr>
  <tr>
	  <th>introduction</th>
	  <th><%= user.introduction %></th>
  </tr>
</table>

<% unless user.guest_user? %>
<div class="row mb-3">
  <%= link_to " プロフィール編集",edit_user_path(current_user),class: "btn btn-outline-secondary btn-block fa-solid fa-user edit_user_#{user.id}" %>
</div>
<div class="row">
  <%= link_to " ブックマーク",liked_parties_user_path(current_user),class: "btn btn-outline-secondary btn-block fa-solid fa-bookmark edit_user_#{user.id}" %>
</div>
<% end %>
