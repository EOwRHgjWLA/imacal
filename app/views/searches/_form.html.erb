<% if user_signed_in? %>
  <%= form_tag search_result_path, method: :get do %>
    <%= text_field_tag :content, @content, placeholder: "検索キーワードを入力してください" %>
    <%= select_tag :model, options_for_select(['Party', 'Tag'], @model), prompt: "モデルを選択してください" %>
    <%= select_tag :method, options_for_select(['完全一致', '部分一致'], @method), prompt: "検索方法を選択してください" %>
    <%= submit_tag "検索" %>
  <% end %>
<% end %>