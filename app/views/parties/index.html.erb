<div class='container px-5 px-sm-0'>
  <%= render 'layouts/errors', obj: @parties %>
  <div class='row'>
    <div class='col-md-3'>
      <h4>ユーザー情報</h4>
      <%= render 'users/info', user: current_user %>
      <h4 class="mt-3">編成を作成する</h4>
      <%= render 'form', party: @party %>
    </div>
    <div class='col-md-8 offset-md-1'>
      <h4>編成一覧</h4>
      <%= render 'index', parties: @parties %>
    </div>
  </div>
</div>